{
	"extends": [
		"stylelint-config-recess-order",
		"stylelint-config-standard",
		"stylelint-config-css-modules",
		"stylelint-config-prettier"
	],
	"plugins": ["stylelint-scss", "stylelint-prettier"],
	"rules": {
		"prettier/prettier": true,
		"at-rule-no-unknown": [
			true,
			{
				"ignoreAtRules": ["mixin", "include"]
			}
		]
	}
}
